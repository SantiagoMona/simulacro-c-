<table class="table table-striped">
    <thead>
        <th># id</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Correo</th>
        <th>Acciones</th>
    </thead>
    @foreach (var item in Model){
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
            <td>@item.BrithDate</td>
            <td> 
                <a class="btn btn-info" asp-action="Details" asp-controller="Users" asp-route-id="">Editar</a>
                <a class="btn btn-warning" asp-action="Details" asp-controller="Users" asp-route-id="@item.Id">Ver Detalles</a>
                <a class="btn btn-danger" asp-action="Details" asp-controller="Users" asp-route-id="">Eliminar</a>

            </td>
        </tr>
    }
</table>